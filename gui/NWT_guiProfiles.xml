<?xml version="1.0" encoding="utf-8"?>
<GUIProfiles>

    <Profile name="NWT_inGameMenuContainer" extends="fs25_menuContainer">
    </Profile>

    <Profile name="NWT_inGameMenuSlider" extends="fs25_listSliderBox" with="anchorTopRight">
        <position value="28px -40px" />
        <size value="6px 640px"/>
    </Profile>

    <Profile name="NWT_inGameMenuHeader" extends="baseReference">
        <imageColor value="0 0 0 0" />
        <textFocusedColor value="$preset_fs25_colorMainHighlight" />
        <textHighlightedColor value="$preset_fs25_colorMainHighlight" />
        <textSize value="16px" />
        <textBold value="true" />
        <textUpperCase value="true" />
        <textColor value="$preset_colorWhite_50" />
        <textAlignment value="left" />
        <textDisabledColor value="$preset_colorDisabled" />
    </Profile>

    <Profile name="NWT_inGameMenuHeaderRight" extends="baseReference">
        <imageColor value="0 0 0 0" />
        <textFocusedColor value="$preset_fs25_colorMainHighlight" />
        <textHighlightedColor value="$preset_fs25_colorMainHighlight" />
        <textSize value="16px" />
        <textBold value="true" />
        <textUpperCase value="true" />
        <textColor value="$preset_colorWhite_50" />
        <textAlignment value="right" />
        <textDisabledColor value="$preset_colorDisabled" />
    </Profile>

     <Profile name="NWT_list" extends="emptyPanel" with="anchorStretchingYLeft pivotTopLeft">
         <position value="0px -40px" />
     </Profile>

     <Profile name="NWT_listItem" extends="baseReference" with="anchorTopStretchingX alternating">
         <height value="32px"/>
         <imageColor value="$preset_fs25_colorGrey" />
         <imageSelectedColor value="$preset_fs25_colorMainHighlight" />
         <alternateBackgroundColor value="$preset_fs25_colorGreyDark_50" />
     </Profile>

     <Profile name="NWT_rowCell" extends="baseReference" with="anchorStretchingYLeft">
         <textSize value="16px" />
         <textAlignment value="center" />
     </Profile>

     <Profile name="NWT_rowCellLeft" extends="NWT_rowCell" with="anchorStretchingYLeft">
         <textAlignment value="left" />
     </Profile>
     <Profile name="NWT_rowCellRight" extends="NWT_rowCell" with="anchorStretchingYRight">
         <textAlignment value="right" />
     </Profile>

     <Profile name="NWT_listItemSubTotalLeft" extends="NWT_rowCellLeft" with="anchorStretchingYLeft">
         <textUpperCase value="true" />
         <textBold value="true" />
     </Profile>
     <Profile name="NWT_listItemSubTotalRight" extends="NWT_rowCellRight" with="anchorStretchingYRight">
         <textUpperCase value="true" />
         <textBold value="true" />
     </Profile>


     <Profile name="NWT_menuHeaderText" extends="fs25_textDefault">
         <size value="600px 64px" />
         <position value="100px 10px" />
         <textSize value="42px"/>
         <textBold value="false"/>
         <textUpperCase value="true"/>
         <textDisabledColor value="$preset_colorDisabled"/>
     </Profile>
    <Profile name="NWT_menuIcon" extends="baseReference" with="anchorMiddleCenter">
        <size value="50px 50px" />
        <imageSliceId value="gui.icon_ingameMenu_prices" />
        <imageColor value="$preset_fs25_colorMainDark" />
    </Profile>

    <Profile name="NWT_inGameMenuFooterBox" extends="baseReference" with="anchorBottomStretchingX">
        <size value="100% 100px"/>
    </Profile>

    <Profile name="NWT_inGameMenuFooter" extends="baseReference">
        <imageColor value="0 0 0 0" />
        <textFocusedColor value="$preset_fs25_colorMainHighlight" />
        <textHighlightedColor value="$preset_fs25_colorMainHighlight" />
        <textSize value="16px" />
        <textBold value="true" />
        <textUpperCase value="true" />
        <textColor value="$preset_colorWhite_50" />
        <textAlignment value="left" />
        <textDisabledColor value="$preset_colorDisabled" />
    </Profile>
    <Profile name="NWT_inGameMenuFooterRight" extends="NWT_inGameMenuFooter" with="anchorStretchingYRight">
        <textAlignment value="right" />
    </Profile>


</GUIProfiles>
